#, qt-format
msgctxt "Cli|"
msgid "Do not print any text when using --%1 or --%2."
msgstr "Ne presi ajnan tekston kiam vi uzas --%1 aŭ --%2."

#: src/cli.cpp:53
msgctxt "Cli|"
msgid "Print only translations."
msgstr "Presi nur tradukojn."

#: src/cli.cpp:54
msgctxt "Cli|"
msgid "Print output formatted as JSON."
msgstr "Presi eligon formatite kiel JSON."

