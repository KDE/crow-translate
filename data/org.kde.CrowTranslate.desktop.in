[Desktop Entry]
Actions=TranslateSelectedText;SpeakSelection;SpeakTranslatedSelection;StopSpeaking;Open;CopyTranslatedSelection;TranslateScreenArea;RecognizeScreenArea;
Name=Crow Translate
Name[ca]=Crow Translate
Name[ca@valencia]=Crow Translate
Name[nl]=Crow vertalen
Name[tr]=Crow Çeviri
Name[uk]=Crow Translate
Name[x-test]=xxCrow Translatexx
GenericName=Translator
GenericName[ca]=Traductor
GenericName[ca@valencia]=Traductor
GenericName[nl]=Vertaler
GenericName[tr]=Çevirmen
GenericName[uk]=Перекладач
GenericName[x-test]=xxTranslatorxx
Comment=A simple and lightweight translator that allows to translate and speak the selected text
Comment[ca]=Un traductor senzill i lleuger que permet traduir i pronunciar el text seleccionat
Comment[ca@valencia]=Un traductor senzill i lleuger que permet traduir i llegir el text seleccionat
Comment[nl]=Een eenvoudige en lichtgewicht vertaler die het vertalen en spreken van de geselecteerde tekst biedt
Comment[tr]=Metinleri çevirmenize ve seslendirmenize olanak tanıyan basit ve hafif bir çeviri yazılımı.
Comment[uk]=Проста і невибаглива до ресурсів програма для перекладу, яка дає вам змогу перекладати і озвучувати позначені фрагменти тексту
Comment[x-test]=xxA simple and lightweight translator that allows to translate and speak the selected textxx
Keywords=google;yandex;text-to-speech;
Keywords[ca]=google;yandex;text-to-speech;
Keywords[ca@valencia]=google;yandex;text-to-speech;
Keywords[nl]=google;yandex;tekst-naar-spraak;
Keywords[tr]=google;yandex;metni konuş;
Keywords[uk]=google;yandex;text-to-speech;гугл;яндекс;озвучення;текст;
Keywords[x-test]=xxgooglexx;xxyandexxx;xxtext-to-speechxx;
Exec=crow
Icon=crow-translate
Terminal=false
StartupNotify=false
Type=Application
Categories=Office;Translation;Qt;
StartupWMClass=crow
X-KDE-DBUS-Restricted-Interfaces=org.kde.kwin.Screenshot
X-Purism-FormFactor=Workstation;Mobile;

[Desktop Action TranslateSelectedText]
Exec=@QT_BIN_DIR@/qdbus org.kde.CrowTranslate /org/kde/CrowTranslate/MainWindow translateSelection
Name=Translate selected text
Name[ca]=Tradueix el text seleccionat
Name[ca@valencia]=Traduïx el text seleccionat
Name[nl]=Geselecteerde tekst vertalen
Name[tr]=Seçili Metni Çevir
Name[uk]=Перекласти позначений текст
Name[x-test]=xxTranslate selected textxx
X-KDE-Shortcuts=Ctrl+Alt+E

[Desktop Action SpeakSelection]
Exec=@QT_BIN_DIR@/qdbus org.kde.CrowTranslate /org/kde/CrowTranslate/MainWindow speakSelection
Name=Speak selected text
Name[ca]=Pronuncia el text seleccionat
Name[ca@valencia]=Llig el text seleccionat
Name[nl]=Geselecteerde tekst uitspreken
Name[tr]=Seçili Metni Konuş
Name[uk]=Озвучити позначений текст
Name[x-test]=xxSpeak selected textxx
X-KDE-Shortcuts=Ctrl+Alt+S

[Desktop Action SpeakTranslatedSelection]
Exec=@QT_BIN_DIR@/qdbus org.kde.CrowTranslate /org/kde/CrowTranslate/MainWindow speakTranslatedSelection
